class Solution {
    public int triangleNumber(int[] nums) {
        Arrays.sort(nums);
        int count=0;
        int n=nums.length;
        for(int k=n-1;k>=2;k--){
            int i=0,j=k-1;
            while(i<j){
                if(nums[i]+nums[j]>nums[k]){
                    count+=j-i;
                    j--;
                }
                else{
                    i++;
                }
            }
        }return count;
    }
}
// Time Complexity: O(n^2)
// - Sorting takes O(n log n)
// - Two-pointer traversal for each k runs in O(n) => total O(n^2)

// Space Complexity: O(1)
// - No extra space used beyond input array (in-place sort)
