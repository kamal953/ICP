class Solution {
    public int singleNumber(int[] arr) {
        int n=arr.length;
        int result=0;
        for(int num:arr){
            result^=num;
        }return result;
     
        }
    
}
// Time Complexity: O(n) — Each element is visited once.
// Space Complexity: O(1) — Only a single integer variable is used.
