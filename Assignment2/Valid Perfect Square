class Solution {
    public boolean isPerfectSquare(int num) {
     
      int low=1, high= num;
      while(low<= high){
         int mid= (low + high)/2;
        if((long)mid* mid== num) return true;
        else if((long)mid* mid > num) high= mid-1;
        else  low= mid +1;  
      }
      return false;  
    }
}
// Time Complexity: O(log n) — binary search halves the range each iteration
// Space Complexity: O(1) — uses constant extra space for variables
